        <name>DocuGenLLM: Generate Coverage Report</name>
        <onclick>generateDocCoverage();</onclick>
        <order>100</order>
        <script><![CDATA[const DEBUG = false;
try {
	const coverageUtil = new DocuGenLLMCoverageUtils();
	const response = coverageUtil.generateCoverageStats(current.getUniqueValue());

	if(DEBUG) gs.addInfoMessage(JSON.stringify(response));
	
	if(response.status === 200) {
		gs.addInfoMessage('[DocuGenLLM: Generate Coverage Report] Documentation Coverage has been generated!');
		gs.addInfoMessage(`<a href='${ response.message.link }'>${ response.message.title }</a>`);
	} else {
		gs.addErrorMessage(response.message);
	}

	action.setRedirectURL(current);
} catch(e) {
	gs.error(`[DocuGenLLM: Generate Coverage Report] Error ${ e }`);
	gs.addErrorMessage(`[DocuGenLLM: Generate Coverage Report] Error: ${ e.message }`);
}]]></script>
