        <script><![CDATA[function navToLink() {
	try {
		var recordSysId = g_form.getUniqueValue();
		var docAjax = new GlideAjax('ClientActionsHandler');
		docAjax.addParam('sysparm_name', 'getDocumentationLink');
		docAjax.addParam('sysparm_recordSysId', recordSysId);
		docAjax.getXML(function(response) {
			var answer = response.responseXML.documentElement.getAttribute("answer");

			window.open(answer);
		});
	} catch(e) {
		g_form.addErrorMessage('[DocuGenLLM Show Documentation] Error: ' + e.message);
	}
}]]></script>
