        <name>DocuGenLLM: Generate Code Annotation</name>
        <onclick/>
        <order>102</order>
        <script><![CDATA[const DEBUG = false;

try {
	const recordName = current.getDisplayValue() ?
		current.getDisplayValue() : current.name ?
			current.name : `${ current.getTableName() }_${ current.getUniqueValue() }`;

	if(DEBUG) gs.addInfoMessage(recordName);

	const docUtil = new DocuGenLLMClientUtils();
	const response = docUtil.generateAnnotation(
		current.getUniqueValue(),
		recordName,
		current.getTableName()
	);

	if(DEBUG) gs.addInfoMessage(JSON.stringify(response));
	
	if(response.status === 200) {
		gs.addInfoMessage('[DocuGenLLM: Generate Code Annotation] Code annotation has been generated! Please note that this feature is in development, so there may be errors or formatting issues.');
		gs.addInfoMessage(`<a href='${ response.message.link }'>${ response.message.title }</a>`);
	} else {
		gs.addErrorMessage(response.message);
	}

	action.setRedirectURL(current);
} catch(e) {
	gs.log(`[DocuGenLLM: Generate Code Annotation] Error ${ e }`);
	gs.addErrorMessage(`[DocuGenLLM: Generate Code Annotation] Error: ${ e.message }`);
}]]></script>
