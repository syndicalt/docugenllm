        <name>Set as Default</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[try {
	current.update();
	
	const modelsGr = new GlideRecord('u_models');
	modelsGr.addQuery('u_default', true);
	modelsGr.query();
	modelsGr.setValue('u_default', false);
	modelsGr.updateMultiple();

	modelsGr.initialize();
	modelsGr.addQuery('sys_id', current.getUniqueValue());
	modelsGr.query();

	if(modelsGr.next()) {
		modelsGr.setValue('u_default', true);
		modelsGr.update();
	}
} catch(e) {
	gs.addErrorMessage(`[DocuGenLLM.UI_Action.set_as_default] Error: ${ e.message }`);
}]]></script>
