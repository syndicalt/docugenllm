        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	try {
		const countAppFiles = current.u_total_app_files;
		const countTables = current.u_app_tables;
		
		current.u_documentation_coverage = `${((current.u_total_documentations / countAppFiles) * 100).toFixed(2)}%`;
		current.u_annotation_coverage = `${((current.u_total_annotations / countAppFiles) * 100).toFixed(2)}%`;
		current.u_code_review_coverage = `${((current.u_total_code_reviews / countAppFiles) * 100).toFixed(2)}%`;
		current.u_table_doc_coverage = `${((current.u_total_tables / countTables) * 100).toFixed(2)}%`;

		current.update();
	} catch(e) {
		gs.error(`[DocuGenLLM Coverage Reports.Data Policies - Calculations] Error: ${e.message}\n Stack: ${e.stack}`);
	}
})(current, previous);]]></script>
